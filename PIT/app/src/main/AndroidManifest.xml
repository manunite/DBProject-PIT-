<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DBProject.heo.pit" >
    <uses-permission android:name="android.permission.READ_CALENDAR"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".Manager.Login_Main"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Activity.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.JoinActivity"></activity>
        <activity android:name=".Activity.MainActivity"></activity>
        <activity
            android:name=".Activity.CategoryActivity">
            <category android:name="android.intent.category.DEFAULT"/>
        </activity>
        <activity android:name=".Activity.CateWizard"></activity>
        <activity android:name=".Activity.ProjectTimeline"></activity>
        <activity android:name=".Activity.ToDoWizard"></activity>
        <activity android:name=".Activity.ToDoWizard_Delete"></activity>
        <activity android:name=".Activity.CateWizard_Delete"></activity>
        <activity android:name=".Activity.ToDoWizard_Modify"></activity>
        <activity android:name=".Activity.ProjectWizard"></activity>
        <activity android:name=".Activity.ProjectWizard_Modify"></activity>
        <activity android:name=".Activity.ProjectWizard_Delete"></activity>
        <activity android:name=".Activity.CateWizard_Modify_List"></activity>
        <activity android:name=".Activity.CateWizard_Modify"></activity>
        <activity android:name=".Activity.UserWizard"></activity>
        <activity android:name=".Activity.adminLoginActivity"></activity>
    </application>


</manifest>
